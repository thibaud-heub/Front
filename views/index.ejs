<!DOCTYPE html>
<html>

<head>
  <title>
    Users list
  </title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
  <h1>
    <u>Users list :</u>
  </h1>
  <br>
  <button onclick="location.href='add'">Add a user</button>
  <br>
  <br>
  <table>
    <thead>
      <tr>
        <td>#</td>
        <td>Email</td>
        <td>First name</td>
        <td>Last name</td>
        <td>Company</td>
        <td>Created at</td>
        <td>Country</td>
      </tr>
    </thead>
    <tbody>
      <% for (let user of users) { %>
        <tr>
          <td>
            <%= user.id %>
          </td>
          <td>
            <%= user.email %>
          </td>
          <td>
            <%= user.first %>
          </td>
          <td>
            <%= user.last %>
          </td>
          <td>
            <%= user.company %>
          </td>
          <td>
            <%= user.created_at %>
          </td>
          <td>
            <%= user.country %>
          </td>
          <td><button>Edit</button></td>
        </tr>
        <% } %>
    </tbody>
  </table>
</body>

</html>